    "paths": {
        "/api/v1/namespaces/{namespace}/sinks/{name}": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Get"
                ],
                "summary": "List sinks",
                "parameters": [
                    {
                        "type": "string",
                        "description": "namespace name",
                        "name": "namespace",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "sink name",
                        "name": "name",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/v1.Sink"
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid parameters",
                        "schema": {
                            "type": "string"
